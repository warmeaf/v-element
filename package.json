{
  "name": "v-element",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/umd/v-element.umd.cjs",
  "module": "./dist/es/v-element.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/v-element.js",
      "require": "./dist/umd/v-element.umd.cjs",
      "types": "./dist/types/src/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "style": "./dist/index.css",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-es build-umd && pnpm move-style",
    "preview": "vite preview",
    "build-umd": "vite build --config vite.config.umd.ts",
    "build-es": "vite build --config vite.config.es.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint src --fix",
    "format": "prettier --write \"./**/*.{html,vue,js,json,md}\"",
    "postinstall": "patch-package",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.5.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^18.19.39",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.6.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.7",
    "move-file-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.2.1",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.38",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-for": "^2.1.1",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.1.6",
    "typescript-eslint": "^7.14.1",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "1.0.0-rc.31",
    "vitest": "^1.6.0",
    "vue": "^3.4.30",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^1.8.27"
  },
  "volta": {
    "node": "20.12.2"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
    ]
  }
}
